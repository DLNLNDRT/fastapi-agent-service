# FastAPI Agent Service Environment Variables
# Copy this file to .env and fill in your actual values

# Authentication credentials (required)
AGENT_API_USERNAME=admin
AGENT_API_PASSWORD=password123

# OpenAI API Key (optional - for AI agent functionality)
# Get your key from: https://platform.openai.com/api-keys
OPENAI_API_KEY=your_openai_api_key_here

# OpenAI Model (optional - defaults to gpt-4o-mini)
OPENAI_MODEL=gpt-4o-mini

# Langfuse Monitoring (optional)
# Get your keys from: https://cloud.langfuse.com
LANGFUSE_PUBLIC_KEY=your_langfuse_public_key
LANGFUSE_SECRET_KEY=your_langfuse_secret_key
LANGFUSE_HOST=https://cloud.langfuse.com
LANGFUSE_TRACING_ENVIRONMENT=development

# Tavily Search API Key (optional - for web search functionality)
TAVILY_API_KEY=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
